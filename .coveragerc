[run]
branch = True

omit =
    manage.py
    venv/*
    */tests/*
    */__init__.py
    *app.py
    */types.py
    */django/*
    */settings/*
    */static/*
    *asgi.py
    *urls.py
    *wsgi.py
    *admin.py
    */migrations/*
    */management/*

source = .

[report]
fail_under = 95
ignore_errors = False
show_missing = True

[html]
directory = coverage