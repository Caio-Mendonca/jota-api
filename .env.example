# .env.example

# Chave secreta do Django - altere para um valor seguro em produção
SECRET_KEY=django-insecure-your-secret-key

# Debug mode (True ou False)
DEBUG=True

# Hosts permitidos (separados por vírgula)
ALLOWED_HOSTS=localhost,127.0.0.1

# Origens confiáveis para CSRF (deve conter o esquema http:// ou https://)
CSRF_TRUSTED_ORIGINS=http://localhost:8000

# Configuração do banco de dados PostgreSQL
DBENGINE=django.db.backends.postgresql
DBNAME=jota_dev
DBUSER=JotaUser
DBPASSWORD=JotaUserPassword
DBHOST=127.0.0.1
DBPORT=5438
